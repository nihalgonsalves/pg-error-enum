{
  "name": "pg-error-enum",
  "version": "2.0.1",
  "description": "TypeScript Enum for Postgres Errors with no runtime dependencies",
  "homepage": "https://github.com/nihalgonsalves/pg-error-enum#readme",
  "bugs": {
    "url": "https://github.com/nihalgonsalves/pg-error-enum/issues"
  },
  "license": "MIT",
  "author": "Nihal Gonsalves <nihal@nihalgonsalves.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nihalgonsalves/pg-error-enum.git"
  },
  "files": [
    "dist/**/*"
  ],
  "type": "module",
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "publishConfig": {
    "provenance": true
  },
  "scripts": {
    "sync": "node bin/sync.ts",
    "clean": "rm -rf ./dist/",
    "typecheck": "tsc --noEmit --project tsconfig.json && tsc --noEmit --project tsconfig.build.json",
    "build": "tsc --build tsconfig.build.json",
    "lint:check": "oxlint --type-aware --report-unused-disable-directives",
    "format:check": "oxfmt --check",
    "format:write": "oxfmt",
    "prepublishOnly": "npm run build",
    "test": "node bin/test.js"
  },
  "devDependencies": {
    "@nihalgonsalves/esconfig": "^0.14.0",
    "@types/node": "^22",
    "knip": "^5.82.1",
    "lefthook": "^2.0.16",
    "oxfmt": "^0.33.0",
    "oxlint": "^1.48.0",
    "oxlint-tsgolint": "^0.14.0",
    "typescript": "^5.9.3",
    "zod": "^4.3.6"
  },
  "engines": {
    "node": "^22.12.0 || >24.0.0"
  },
  "packageManager": "pnpm@10.29.3+sha512.498e1fb4cca5aa06c1dcf2611e6fafc50972ffe7189998c409e90de74566444298ffe43e6cd2acdc775ba1aa7cc5e092a8b7054c811ba8c5770f84693d33d2dc"
}
